{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-console": [
      "warn",
      {
        "allow": [
          "warn"
        ]
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
